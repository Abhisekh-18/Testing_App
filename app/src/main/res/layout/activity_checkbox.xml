<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".checkbox">

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="189dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="Butter Chicken"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="189dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="Mutton Kimma"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/checkBox2"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="@+id/checkBox2"
        app:layout_constraintTop_toBottomOf="@+id/checkBox2"
        app:layout_constraintVertical_bias="0.051" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="189dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="Mutton Lababdar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/checkBox3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/checkBox3"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3"
        app:layout_constraintVertical_bias="0.045" />

    <Button
        android:id="@+id/button17"
        android:layout_width="186dp"
        android:layout_height="59dp"
        android:onClick="show_checked"
        android:text="Show the checked items"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/checkBox4"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/checkBox4"
        app:layout_constraintTop_toBottomOf="@+id/checkBox4"
        app:layout_constraintVertical_bias="0.057" />

    <RadioGroup
        android:id="@+id/rg"
        android:layout_width="222dp"
        android:layout_height="85dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button17"
        app:layout_constraintVertical_bias="0.122">

        <RadioButton
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:id="@+id/r1"
            android:text="Bad">

        </RadioButton>

        <RadioButton
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:id="@+id/r2"
            android:text="Average">

        </RadioButton>

        <RadioButton
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:id="@+id/r3"
            android:text="Good">

        </RadioButton>
    </RadioGroup>

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Give Feedback"
        android:onClick="rating"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/rg"
        app:layout_constraintStart_toStartOf="@+id/rg"
        app:layout_constraintTop_toBottomOf="@+id/rg"
        app:layout_constraintVertical_bias="0.08" />


</androidx.constraintlayout.widget.ConstraintLayout>